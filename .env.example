APP_BASE_URL=http://localhost:3810
BETTER_AUTH_SECRET=replace-with-a-random-32+-character-secret
GITHUB_CLIENT_ID=your-github-oauth-client-id
GITHUB_CLIENT_SECRET=your-github-oauth-client-secret
# Optional in development: if set, /projects can load without OAuth session using this PAT
GITHUB_DEV_TOKEN=ghp_your_local_dev_token

# Optional: enable destructive GitHub lifecycle tests (create/list/delete repository)
RUN_GITHUB_LIFECYCLE_TESTS=false
ALLOW_GITHUB_DESTRUCTIVE_TESTS=false
GITHUB_TEST_REPO_PREFIX=github-light-e2e

# Optional: override TanStack devtools event bus port in dev
TANSTACK_DEVTOOLS_PORT=42070
